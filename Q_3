#include <stdio.h>
#include <stdlib.h>
#include <locale.h>




int main() {
	
    setlocale(LC_ALL, "Portuguese_Brazil");

	int numero;

    scanf("%d",&numero);
    
    if (numero %2==0){
    printf("o numero é par\n");
	}
	
	else{	
	printf("o numero é impar\n");
	
	}
	
  system("pause");
return 0;
}
