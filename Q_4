#include <stdio.h>
#include <stdlib.h>
#include <locale.h>




int main() {
	
    setlocale(LC_ALL, "Portuguese_Brazil");

	int numero;

    scanf("%d",&numero);
    
    if(numero % 5==0){
    printf("é multiplo de 5\n");
	}
	
	else{	
	printf("não é multiplo de 5\n");
	
	}
	
  system("pause");
return 0;
}
